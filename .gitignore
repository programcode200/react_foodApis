# Ignore node_modules (should not be committed)
node_modules/

# Ignore Parcel cache (can be regenerated)
.parcel-cache/

# Ignore build output (Parcel generates /dist for production builds)
dist/

# Ignore environment variables (if any)
.env
.env.local
.env.development
.env.production
.env.test

# Ignore test coverage reports (if you're using Jest or other test tools)
coverage/
coverage.parcel-cache/

# Ignore personal notes or temporary files
Notes.txt

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore editor & IDE-specific files
.vscode/
.idea/
*.swp
*.swo

# Ignore log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore Git LFS pointers (if using LFS, but don't track node_modules)
.gitattributes
